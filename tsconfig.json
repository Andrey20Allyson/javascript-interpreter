{
  "compilerOptions": {
    "target": "ES2024",
    "module": "CommonJS",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@parsing/*": ["parsing/*"],
      "@runtime/*": ["runtime/*"],
      "@utils/*": ["utils/*"],
      "@lexical-analysis/*": ["lexical-analysis/*"]
    },
    "skipDefaultLibCheck": true,
    "esModuleInterop": true
  }
}
